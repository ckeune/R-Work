required_libraries <- c('ggplot2',
'reshape2',
'stringr',
'lubridate',
'dplyr',
'foreign',
'xlsx')
#Install/load required libraries
for (lib_name in required_libraries){
if (!require(lib_name, character.only = T))
install.packages(lib_name)
library(lib_name, character.only = T)
}
